{
  "manifest_version": 3,
  "name": "Wistia Engagement Analyzer",
  "version": "1.0",
  "description": "Analyze Wistia video engagement metrics against benchmarks",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://*.wistia.com/*",
    "https://*.wistia.net/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
} 